{
  "name": "framespaces-web",
  "version": "0.0.1",
  "description": "Web application of framespaces.com",
  "homepage": "http://framespaces.com",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "svn",
    "url": "https://subversion.assembla.com/svn/framespaces/trunk/"
  },
  "dependencies": {
    "ajv": "^4.1.3",
    "async": "^2.0.0-rc.5",
    "compute-mean": "^3.0.0",
    "compute-stdev": "^1.0.0",
    "ejs": "^2.4.2",
    "express": "^4.13.4",
    "html.js": "^0.12.1",
    "keycode": "^2.1.1",
    "kld-affine": "^0.1.0",
    "lodash": "^4",
    "loglevel": "^1.4.1",
    "pass-error": "^0.1.2",
    "request": "^2.72.0",
    "snapsvg": "^0.4",
    "socket.io": "^1.4.6",
    "svg-intersections": "https://github.com/framespaces/svg-intersections.git",
    "svg-points": "^2.0.1",
    "taffydb": "^2.7.2"
  },
  "devDependencies": {
    "browserify": "^13",
    "chai": "^3",
    "envify": "^3.4.1",
    "jsdom": "9.2.1",
    "jsdom-global": "2.0.0",
    "minifyify": "^7.3.3",
    "mocha": "^2"
  },
  "scripts": {
    "start": "node server/index.js",
    "stop": "pkill -SIGINT Framespaces",
    "test": "./node_modules/.bin/mocha test",
    "start:dev": "npm run build:dev && LOG_LEVEL=DEBUG npm start",
    "restart:dev": "npm run build:dev && LOG_LEVEL=DEBUG npm restart",
    "copy:dist": "cp -R web/* dist/",
    "bundle:dev": "LOG_LEVEL=DEBUG ./node_modules/browserify/bin/cmd.js client/index.js -t envify > dist/bundle.js",
    "bundle:dist": "./node_modules/browserify/bin/cmd.js client/index.js -d -p [minifyify --map bundle.js.map --output dist/bundle.js.map] -t envify > dist/bundle.js",
    "build:dist": "npm test && npm run copy:dist && npm run bundle:dist",
    "build:dev": "npm test && npm run copy:dist && npm run bundle:dev"
  }
}
