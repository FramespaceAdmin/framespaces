{
  "name": "framespaces-web",
  "version": "0.0.1",
  "description": "Web application of framespaces.com",
  "homepage": "http://framespaces.com",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "svn",
    "url": "https://github.com/FramespaceAdmin/framespaces"
  },
  "dependencies": {
    "ably": "^0.8.34",
    "ajv": "^4.1.3",
    "aliasify": "^2.0.0",
    "async": "^2.0.0-rc.5",
    "body-parser": "^1.15.2",
    "browserify": "^13",
    "compute-stdev": "^1.0.0",
    "config": "^1.21.0",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.4.2",
    "express": "^4.13.4",
    "html.js": "^0.12.1",
    "js-cookie": "^2.1.3",
    "jsonwebtoken": "^7.1.9",
    "jwt-decode": "^2.1.0",
    "keycode": "^2.1.1",
    "kld-affine": "^2.0.0",
    "lodash": "^4",
    "loglevel": "^1.4.1",
    "minifyify": "^7.3.3",
    "pass-error": "^0.1.2",
    "request": "^2.72.0",
    "snapsvg": "^0.4",
    "socket.io": "^1.4.6",
    "svg-intersections": "git+https://github.com/framespaces/svg-intersections.git#master",
    "svg-points": "^2.0.1",
    "taffydb": "^2.7.2",
    "url-join": "^1.1.0",
    "xhr": "^2.2.2",
    "yavl": "^0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "jsdom": "^9.2.1",
    "jsdom-global": "^2.0.0",
    "mocha": "^2.5.3",
    "proxyquire": "^1.7.10"
  },
  "aliasify": "./client/modules.js",
  "scripts": {
    "postinstall": "npm run build:dist",
    "start": "node server/index.js",
    "stop": "pkill -SIGINT Framespaces",
    "test": "mocha test",
    "start:dev": "npm run build:dev && NODE_ENV=development node server/index.js",
    "restart:dev": "npm run build:dev && npm stop && NODE_ENV=development node server/index.js",
    "copy:dist": "mkdir -p dist && cp -R web/* dist/",
    "bundle:dev": "NODE_ENV=development ./node_modules/browserify/bin/cmd.js client -t aliasify > dist/bundle.js",
    "bundle:dist": "./node_modules/browserify/bin/cmd.js client -d -p [minifyify --map bundle.js.map --output dist/bundle.js.map] -t aliasify > dist/bundle.js",
    "build:dist": "npm run copy:dist && npm run bundle:dist",
    "build:dev": "npm test && npm run copy:dist && npm run bundle:dev"
  }
}
